package Interview;

class Programming {
	public static void main(String[] args) {

		int sum = 0;
		for (int i = 1; i <= 100; i++) {
			sum = sum + i;
		}
		System.out.println("1~100까지 더한 수 : " + sum);
	}
		int sum = 0;
		for (int i = 1; i <= 100; i++) {
			sum = sum + i;
		System.out.println(sum);
	}
		
}
