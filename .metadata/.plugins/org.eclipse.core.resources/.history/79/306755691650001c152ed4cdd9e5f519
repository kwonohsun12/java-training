package Interview;

class Programming {
	public static void main(String[] args) {

		int sum = 0;
		for (int i = 1; i <= 100; i++) {

			sum = sum + i;
		}
		System.out.println("1~100까지 더한 수 : " + sum);

		for (int i = 1; i <= 9; i++) {
			for (int j = 1; j <=9; j++) {
				System.out.println(i + "*" + j + "=" + (i * j));
			}
		}
		
	}
}
