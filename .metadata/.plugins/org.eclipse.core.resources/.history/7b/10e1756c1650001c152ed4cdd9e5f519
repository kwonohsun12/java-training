package Interview;

class Programming {
	public static void main(String[] args) {
		
		//1~100까지 더하기
		int sum = 0;
		for (int i = 1; i <= 100; i++) {

			sum = sum + i;
		}
		System.out.println("1~100까지 더한 수 : " + sum);

		
		// for문을 이요한 구구단 만들기
		for (int i = 1; i <= 9; i++) {
			sys
			for (int j = 1; j <=9; j++) {
				System.out.println(i + "단");
				System.out.println(i + "*" + j + "=" + (i * j));
			}
		}
		
		// 구구단 3단에서 4의 배수를 제외하고 출력
		
		
		
	}
}
